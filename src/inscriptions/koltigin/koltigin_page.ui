<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>
  <template class="PageKoltigin" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="AdwCarousel" id="koltigin_carousel">
        <property name="allow-scroll-wheel">false</property>
        <child>
          <object class="KoltiginSlideSouth" id="koltigin_slide_south"></object>
        </child>
        <child>
          <object class="KoltiginSlideEast" id="koltigin_slide_east"></object>
        </child>
        <child>
          <object class="KoltiginSlideNorth" id="koltigin_slide_north"></object>
        </child>
        <child>
          <object class="KoltiginSlideNortheast" id="koltigin_slide_northeast"></object>
        </child>
        <child>
          <object class="KoltiginSlideSoutheast" id="koltigin_slide_southeast"></object>
        </child>
        <child>
          <object class="KoltiginSlideSouthwest" id="koltigin_slide_southwest"></object>
        </child>
        <child>
          <object class="KoltiginSlideWest" id="koltigin_slide_west"></object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwCarouselIndicatorDots" id="koltigin_carousel_dots">
        <property name="carousel">koltigin_carousel</property>
      </object>
    </child>  
  </template>
</interface>
